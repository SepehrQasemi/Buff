{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "required": [
    "timeframes",
    "symbols",
    "global",
    "per_symbol"
  ],
  "properties": {
    "timeframes": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "symbols": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "global": {
      "$ref": "#/definitions/report"
    },
    "per_symbol": {
      "type": "array",
      "items": {
        "$ref": "#/definitions/report"
      }
    }
  },
  "definitions": {
    "gap": {
      "type": "object",
      "required": [
        "start",
        "end",
        "missing_bars"
      ],
      "properties": {
        "start": {
          "type": "string"
        },
        "end": {
          "type": "string"
        },
        "missing_bars": {
          "type": "integer"
        },
        "symbol": {
          "type": "string"
        },
        "timeframe": {
          "type": "string"
        }
      },
      "additionalProperties": true
    },
    "report": {
      "type": "object",
      "required": [
        "symbol",
        "timeframe",
        "rows_total",
        "first_ts",
        "last_ts",
        "expected_bars_count",
        "missing_bars_count",
        "missing_ratio",
        "gaps_count",
        "gap_ranges",
        "duplicates_count",
        "zero_volume_bars_count",
        "high_lt_low_count",
        "negative_price_count",
        "nan_count",
        "sha256"
      ],
      "properties": {
        "symbol": {
          "type": "string"
        },
        "timeframe": {
          "type": "string"
        },
        "rows_total": {
          "type": "integer"
        },
        "first_ts": {
          "type": "string"
        },
        "last_ts": {
          "type": "string"
        },
        "expected_bars_count": {
          "type": "integer"
        },
        "missing_bars_count": {
          "type": "integer"
        },
        "missing_ratio": {
          "type": "number"
        },
        "gaps_count": {
          "type": "integer"
        },
        "gap_ranges": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/gap"
          }
        },
        "duplicates_count": {
          "type": "integer"
        },
        "zero_volume_bars_count": {
          "type": "integer"
        },
        "high_lt_low_count": {
          "type": "integer"
        },
        "negative_price_count": {
          "type": "integer"
        },
        "nan_count": {
          "type": "integer"
        },
        "sha256": {
          "type": "string"
        }
      },
      "additionalProperties": true
    }
  },
  "additionalProperties": true
}